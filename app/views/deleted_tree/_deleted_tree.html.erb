<div id="<%= dom_id(deleted_tree) %>" class="deleted-tree deleted-tree-<%= deleted_tree.state %> ui-tabs-panel ui-widget-content ui-corner-bottom ui-tabs-hide">
  <div class="tree-background">
    
    <div class="breadcrumbs">
      <ul>
        <li>Deleted Names</li>
      </ul>
    </div>

    <%= render :partial => 'shared/tree_searches.html' %>

    <div id="toolbar-deleted" class="toolbar ddsmoothmenu clearfix">
      <ul class="topnav">
        <li><a href="#">View</a>
            <ul class="subnav">
              <li><a href="#" class="nav-refresh-tree">Refresh tree</a></li>
              <li><a href="#" class="nav-collapse-tree">Collapse tree</a></li>
            </ul>
        </li>
      </ul>
    </div>

    <div class="tree-container deleted-tree-container" data-database-id="<%= deleted_tree.id %>">
      <div id=<%= dom_id(deleted_tree, "container_for") %>></div>
    </div>

    <%= render :partial => 'master_trees/metadata.html' %>
  </div>
</div>