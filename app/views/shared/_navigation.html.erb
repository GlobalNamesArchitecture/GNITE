<!-- Start of Sidebar -->
<div id="navbar">
  <div id="logo"><img src="/images/logo-nav.png"/></div>
  <div id="nav">
    <ul>
      <li id="active">
        <%= link_to trees_path do %>
          <img class="icon hover" src="/images/tree-active.png"/>Master Trees
        <% end %>
        <ul>
          <% current_user.trees.each do |tree| %>
            <li><%= link_to tree.title, tree_url(tree.id) %></li>
        <% end %>
        </ul>
      </li>
      <li>
        <%= link_to edit_user_path(current_user.id) do %>
          <img class="icon" src="/images/profile-nonactive.png"/>Profile Settings
        <% end %>
      </li>
      <!--
      <li><a href="#"><img class="icon" src="/images/settings-nonactive.png"/>Settings</a></li>
      <li><a href="karma.html"><img class="icon" src="/images/karma-nonactive.png"/>Karma</a></li>
      -->
    </ul>
  </div>
</div>
<!-- End of Sidebar -->
