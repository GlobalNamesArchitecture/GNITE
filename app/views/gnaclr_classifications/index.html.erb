<%= content_for :panel_header do %>
    <li>
      <%= link_to 'New Import', new_master_tree_import_path(@master_tree), :class => 'ajax-load-new-tab' %>
    </li>
    <li>
      GNACLR
    </li>
<% end %>
<ul id="tree-import-list" class="list">
  <% @gnaclr_classifications.each do |classification| %>
    <li>
      <%= link_to master_tree_gnaclr_classification_path(@master_tree, classification), :class => "ajax-load-new-tab" do %>
        <div class="btn-img result">
          <div class="content">
            <h2><%= classification.title %></h2>
            <% if classification.description.present? %>
              <p><%= classification.description %></p>
            <% end %>
          </div>
        </div>
        <div class="clearfix"></div>
      <% end %>
    </li>
  <% end %>
</ul>
